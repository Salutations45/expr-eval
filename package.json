{
  "name": "expr-eval",
  "version": "2.0.2",
  "description": "Mathematical expression evaluator",
  "main": "dist/bundle.js",
  "module": "dist/index.mjs",
  "typings": "parser.d.ts",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-semistandard": "^17.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rollup": "^2.77.4"
  },
  "scripts": {
    "test": "npm run build && mocha",
    "coverage": "npm run build && nyc --reporter=lcov --reporter=text-summary mocha",
    "format": "eslint index.js src test rollup.config.js --fix",
    "lint": "eslint index.js src test rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "build": "rollup -c rollup.config.js && rollup -c rollup-esm.config.js"
  },
  "files": [
    "dist/",
    "parser.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silentmatt/expr-eval.git"
  },
  "keywords": [
    "expression",
    "math",
    "evaluate",
    "eval",
    "function",
    "parser"
  ],
  "author": "Matthew Crumley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silentmatt/expr-eval/issues"
  },
  "homepage": "https://github.com/silentmatt/expr-eval#readme"
}
